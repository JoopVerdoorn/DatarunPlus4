        <listEntry value="2">@Strings.lDistLastLap</listEntry>
      </settingConfig>
    </setting>
    <setting propertyKey="@Properties.pHeartratefield" title="@Strings.lHeartratefield">
      <settingConfig type="list" required="true">
        <listEntry value="0">@Strings.lHeartrate</listEntry>
        <listEntry value="1">@Strings.lHeartrateZone</listEntry>
        <listEntry value="2">@Strings.lNoheartrate</listEntry>
      </settingConfig>
    </setting>
    <setting propertyKey="@Properties.pPacefield" title="@Strings.lPacefield">
      <settingConfig type="list" required="true">
        <listEntry value="0">@Strings.lPaceperkm</listEntry>
